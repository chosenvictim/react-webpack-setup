{
	"name": "react-webpack-setup",
	"version": "1.0.0",
	"description": "React Webpack Babel setup for kickstarting project in no time",
	"main": "server.js",
	"scripts": {
		"build": "NODE_ENV=production webpack -p --config ./webpack.config.prod.js",
		"start": "node server.js",
		"dev": "webpack-dev-server --progress --colors --config ./webpack.config.dev.js"
	},
