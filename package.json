	"license": "MIT",
	"devDependencies": {
		"babel-core": "6.17.0",
		"babel-loader": "6.2.5",
		"babel-preset-es2015": "6.16.0",
		"babel-preset-react": "6.16.0",
		"chunk-manifest-webpack-plugin": "0.1.0",
		"css-loader": "0.25.0",
		"extract-text-webpack-plugin": "1.0.1",
		"file-loader": "0.9.0",
		"html-webpack-plugin": "2.22.0",
		"json-loader": "0.5.4",
		"node-sass": "3.10.1",
		"react-dom": "15.3.2",
		"sass-loader": "4.0.2",
		"style-loader": "0.13.1",
		"url-loader": "0.5.7",
		"webpack": "1.13.2",
		"webpack-dev-server": "1.16.1",
		"webpack-manifest-plugin": "1.0.1"
	},
	"dependencies": {
		"kcors": "1.3.0",
		"koa": "1.2.4",
		"koa-compress": "1.0.9",
		"koa-mount": "1.3.0",
		"koa-router": "5.4.0",
		"koa-static": "2.0.0",
		"koa-validation": "0.1.9",
		"path": "0.12.7",
