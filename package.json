    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "svg-sprite-loader": "0.0.31",
    "svgo-loader": "1.1.2",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.1",
    "webpack-manifest-plugin": "1.0.1"
  },
  "dependencies": {
    "koa": "1.2.4",
    "koa-static": "2.0.0",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.6",
