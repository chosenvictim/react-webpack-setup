{
  "name": "react-webpack-setup",
  "version": "1.0.0",
  "description": "React Webpack Babel setup for kickstarting reactjs project in no time",
  "main": "server.js",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --config ./webpack.config.prod.js",
    "start": "node server.js",
    "dev": "webpack-dev-server --progress --colors --config ./webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shek8034/react-webpack-setup"
  },
  "author": "Shekhar Kumar <shekhar.mnnit10@gmail.com> (http://iamshek.online/)",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "webpack",
    "react webpack setup",
    "react starter",
    "react router",
    "babel",
    "koa"
  ],
  "bugs": {
    "url": "https://github.com/shek8034/react-webpack-setup/issues"
  },
  "devDependencies": {
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "chunk-manifest-webpack-plugin": "0.1.0",
    "css-loader": "0.25.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "json-loader": "0.5.4",
    "node-sass": "3.10.1",
